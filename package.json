{
  "name": "glossary",
  "version": "1.0.0",
  "description": "Glossary of MetOcean terms",
  "main": "index.js",
  "scripts": {
    "js": "./node_modules/.bin/browserify ./client.js | ./node_modules/.bin/uglifyjs - --screw-ie8 -mc -o ./dist/bundle.js",
    "wjs": "./node_modules/.bin/watchify ./client.js -o ./dist/bundle.js",
    "css": "./node_modules/.bin/stylus --include-css style/index.styl -o ./dist/bundle.css",
    "wcss": "./node_modules/.bin/stylus --include-css --watch style/index.styl -o ./dist/bundle.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/metocean/glossary.git"
  },
  "keywords": [
    "glossary",
    "metocean"
  ],
  "author": "MetOcean Solutions Ltd <t.coats@metocean.co.nz>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/metocean/glossary/issues"
  },
  "homepage": "https://github.com/metocean/glossary#readme",
  "dependencies": {
    "katex": "0.5.1",
    "kramed": "0.5.5",
    "odo-async": "1.0.2",
    "odo-relay": "2.4.0",
    "odo-route": "1.3.1",
    "odojs": "4.4.0",
    "odoql": "7.10.0",
    "odoql-exe": "1.6.2",
    "odoql-store": "0.1.0",
    "superagent": "1.7.2"
  },
  "devDependencies": {
    "browserify": "13.0.0",
    "exorcist": "0.4.0",
    "stylus": "0.54.0",
    "uglify-js": "2.6.2",
    "watchify": "3.7.0"
  }
}
